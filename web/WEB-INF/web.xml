<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>
            LoginController
        </welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>Controller.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminController</servlet-name>
        <servlet-class>Controller.AdminController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PlanesController</servlet-name>
        <servlet-class>Controller.PlanesController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BuscarController</servlet-name>
        <servlet-class>Controller.BuscarController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SociosController</servlet-name>
        <servlet-class>Controller.SociosController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BuscarSociosController</servlet-name>
        <servlet-class>Controller.BuscarSociosController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AutocompleteController</servlet-name>
        <servlet-class>Controller.AutocompleteController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VentasController</servlet-name>
        <servlet-class>Controller.VentasController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductosController</servlet-name>
        <servlet-class>Controller.ProductosController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CarritoServlet</servlet-name>
        <servlet-class>Controller.CarritoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/LoginController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminController</servlet-name>
        <url-pattern>/AdminController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PlanesController</servlet-name>
        <url-pattern>/PlanesController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BuscarController</servlet-name>
        <url-pattern>/BuscarController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SociosController</servlet-name>
        <url-pattern>/SociosController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BuscarSociosController</servlet-name>
        <url-pattern>/BuscarSociosController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AutocompleteController</servlet-name>
        <url-pattern>/AutocompleteController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VentasController</servlet-name>
        <url-pattern>/VentasController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductosController</servlet-name>
        <url-pattern>/ProductosController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CarritoServlet</servlet-name>
        <url-pattern>/CarritoServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
